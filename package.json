{
  "name": "rankabotmk3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=16.9.0"
  },
  "files": [
    "src/public/config.json"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "prisma:gen": "npx prisma generate --schema src/services/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema src/services/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema src/services/prisma/schema.prisma init",
    "start:js": "node --trace-warnings dist/index.js",
    "start:ts": "ts-node src/index.ts",
    "clean-dist": "rm -rf ./dist/",
    "prebuild": "npm run clean-dist",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts",
    "prod": "npm run build && npm run start:js",
    "watch": "tsc --watch",
    "postinstall": "patch-package",
    "copy-static-assets": "ts-node copy-static-assets.ts",
    "postbuild": "npm run copy-static-assets"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "@discordjs/rest": "^1.0.0",
    "@prisma/client": "^4.1.1",
    "@types/lodash": "^4.14.182",
    "axios": "^0.27.2",
    "chrono-node": "^2.3.9",
    "discord-api-types": "^0.36.2",
    "discord.js": "^14.3.0",
    "discord.js-rate-limiter": "^1.3.2",
    "dotenv": "^16.0.1",
    "jest-create-mock-instance": "^2.0.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.1",
    "node-fetch": "^2",
    "oo-ascii-tree": "^1.62.0",
    "pino": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "pokenode-ts": "^1.16.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.5",
    "@types/node-cron": "^3.0.2",
    "@types/node-fetch": "^2.6.2",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}