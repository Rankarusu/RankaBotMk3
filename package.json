{
  "name": "rankabotmk3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "build": "tsc",
    "prisma:gen": "npx prisma generate --schema src/services/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema src/services/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema src/services/prisma/schema.prisma init",
    "start": "ts-node src/index.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts",
    "start:prod": "npm run build && node dist/index.js",
    "watch": "tsc --watch",
    "postinstall": "patch-package"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "@discordjs/rest": "^0.5.0",
    "@prisma/client": "^4.0.0",
    "@types/lodash": "^4.14.182",
    "chrono-node": "^2.3.8",
    "discord-api-types": "^0.33.5",
    "discord.js": "^13.6.0",
    "discord.js-rate-limiter": "^1.3.2",
    "dotenv": "^16.0.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.1",
    "node-fetch": "^2.6.7",
    "oo-ascii-tree": "^1.61.0",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/node-cron": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2",
    "prisma": "^4.0.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
