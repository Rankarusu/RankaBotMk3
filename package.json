{
  "name": "rankabotmk3",
  "version": "1.3.1",
  "description": "",
  "main": "",
  "engines": {
    "node": ">=16.9.0"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "prisma:gen": "npx prisma generate",
    "prisma:pull": "npx prisma db pull",
    "prisma:migrate:dev": "npx prisma migrate dev init",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "start:ts": "ts-node src/index.ts",
    "clean-dist": "rm -rf ./dist/",
    "prebuild": "npm run clean-dist",
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts",
    "prod": "node dist/index.js",
    "prod:db": "npx prisma migrate deploy && npm run prod",
    "watch": "tsc --watch",
    "postinstall": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "commit": "cz",
    "release": "standard-version --no-verify"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "@discordjs/rest": "^1.0.0",
    "@prisma/client": "^4.1.1",
    "@types/lodash": "^4.14.182",
    "axios": "^0.27.2",
    "chrono-node": "^2.3.9",
    "discord-api-types": "^0.36.2",
    "discord.js": "^14.4.0",
    "discord.js-rate-limiter": "^1.3.2",
    "dotenv": "^16.0.1",
    "jest-create-mock-instance": "^2.0.0",
    "node-cron": "^3.0.1",
    "node-fetch": "^2",
    "oo-ascii-tree": "^1.62.0",
    "patch-package": "^6.4.7",
    "pino": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "pokenode-ts": "^1.17.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.0.5",
    "@types/node-cron": "^3.0.2",
    "@types/node-fetch": "^2.6.2",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "shelljs": "^0.8.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
